							"# Azure storage access info\r\n",
							"blob_account_name = \"azureopendatastorage\"\r\n",
							"blob_container_name = \"laborstatisticscontainer\"\r\n",
							"blob_relative_path = \"laus/\"\r\n",
							"blob_sas_token = r\"\""
						],
						"outputs": [],
						"execution_count": null
					},
					{
						"cell_type": "code",
						"metadata": {
							"jupyter": {
								"source_hidden": false,
								"outputs_hidden": false
